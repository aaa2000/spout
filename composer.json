{
    "name": "portphp/spout",
    "description": "Spout adapter for PortPHP",
    "license": "MIT",
    "homepage": "https://github.com/portphp",
    "authors": [
        {
            "name": "David de Boer",
            "email": "david@ddeboer.nl"
        },
        {
            "name": "Markus Bachmann",
            "email": "markus.bachmann@bachi.biz"
        },
        {
            "name": "Community contributors",
            "homepage": "https://github.com/portphp/spout/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/portphp/portphp/issues",
        "source": "https://github.com/portphp/spout",
        "docs": "http://docs.portphp.org"
    },
    "require": {
        "php": ">=5.5.0",
        "portphp/portphp": "^1.0",
        "box/spout": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Port\\Spout\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Port\\Spout\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
